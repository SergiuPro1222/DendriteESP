coroutine.wrap(QCWS_fake_script)()
local function RVGKHA_fake_script() -- StarterCharacterScripts.LocalScript 
	local script = Instance.new('LocalScript', StarterCharacterScripts)

	local UserInputService = game:GetService("UserInputService")
	
	local gui = script.Parent.Frame
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(RVGKHA_fake_script)()
local function JGDMX_fake_script() -- StarterCharacterScripts.LocalScript 
	local script = Instance.new('LocalScript', StarterCharacterScripts)

	local frame = script.Parent.Frame -- Assuming the frame is a child of the script's parent
	
	local toggleKey = Enum.KeyCode.V -- Change this to the desired key code
	
	local isVisible = false
	
	local function toggleFrame()
		isVisible = not isVisible
		frame.Visible = isVisible
	end
	
	-- Function to handle key presses
	local function onKeyDown(input)
		if input.KeyCode == toggleKey then
			toggleFrame()
		end
	end
	
	-- Connect the key press event
	game:GetService("UserInputService").InputBegan:Connect(onKeyDown)
end
coroutine.wrap(JGDMX_fake_script)()
local function AVVB_fake_script() -- TextButton_6.LocalScript 
	local script = Instance.new('LocalScript', TextButton_6)

	local mainuiu = script.Parent.Parent.Parent.Frame
	local this = script.Parent
	script.Parent.MouseButton1Click:Connect(function()
		if not mainuiu.Visible == true then
			mainuiu.Visible = true 
			this.Text = "Opened"
		else
			mainuiu.Visible = false 
			this.Text = "Closed"
		end
	end)
end
coroutine.wrap(AVVB_fake_script)()
else
print(player.Name .. " is not whitelisted. Denying access.")
player:Kick(player.Name .. " is not whitelisted. Nigga Tried")
end
end
attemptAccess(game.Players.LocalPlayer)
