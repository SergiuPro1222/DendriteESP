local whitelistedHWID = "2f51498a-1525-4030-82b1-f599745270b2"
function checkWhitelist(hwid, key)
    return hwid == whitelistedHWID and key == DendriteKey
end
function getPlayerKey()
    return "darkisnigger111"
end
local function isPlayerWhitelisted(player)
    local HWID = game:GetService("RbxAnalyticsService"):GetClientId()
    local playerKey = getPlayerKey()
    return checkWhitelist(HWID, playerKey)
end
local function attemptAccess(player)
if isPlayerWhitelisted(player) then
