-- Scripts:

local function RRBHE_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local farmingtwo = script.Parent.Parent.cottonmisc
	local settingspl = script.Parent.Parent.cottonsettings
	script.Parent.MouseButton1Click:Connect(function()
		farmingtwo.Visible = true
		settingspl.Visible = false
	end)
end
coroutine.wrap(RRBHE_fake_script)()
local function TKPT_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	local farmingtwo = script.Parent.Parent.cottonmisc
	local settingspl = script.Parent.Parent.cottonsettings
	script.Parent.MouseButton1Click:Connect(function()
		farmingtwo.Visible = false
		settingspl.Visible = true
	end)
end
coroutine.wrap(TKPT_fake_script)()
local function CEEU_fake_script() -- TextButton_3.LocalScript 
	local script = Instance.new('LocalScript', TextButton_3)

	_G.Nitradenpgggggaaxasaautoloot = false
	
	local RunService = game:GetService("RunService")
	local Players = game:GetService("Players")
	local connection = nil
	
	local function checkPrompts()
		if _G.Nitradenpgggggaaxasaautoloot then
			local localPlayer = Players.LocalPlayer
			if localPlayer and localPlayer.Character then
				local playerPosition = localPlayer.Character.PrimaryPart.Position
				for _, player in pairs(Players:GetPlayers()) do
					if player ~= localPlayer then
						local character = player.Character
						if character and character:FindFirstChild("UpperTorso") then
							local upperTorso = character.UpperTorso
							for _, child in pairs(upperTorso:GetChildren()) do
								if child:IsA("ProximityPrompt") and child.Enabled then
									local promptPosition = child.Parent.Position
									local distance = (playerPosition - promptPosition).magnitude
									if distance <= 14 then
										child.HoldDuration = 0
									end
								end
							end
						end
					end
				end
			end
		end
	end
	
	script.Parent.MouseButton1Click:Connect(function()
		if _G.Nitradenpgggggaaxasaautoloot == false then
			_G.Nitradenpgggggaaxasaautoloot = true
			script.Parent.Parent.checkerudeemn.BackgroundColor3 = Color3.fromRGB(255, 170, 0)
			connection = RunService.Stepped:Connect(checkPrompts)
		else
			_G.Nitradenpgggggaaxasaautoloot = false
			script.Parent.Parent.checkerudeemn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
			if connection then
				connection:Disconnect()
				connection = nil
			end
		end
	end)
	
end
coroutine.wrap(CEEU_fake_script)()
local function WUNGFCC_fake_script() -- TextButton_4.LocalScript 
	local script = Instance.new('LocalScript', TextButton_4)

	_G.Nitradenpgggggasaantf = false
	local vu = game:GetService("VirtualUser")
	local toggleAntiAFK = false
	local antiAFKConnection
	
	function antiAFKAction()
		vu:Button2Down(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
		wait(1)
		vu:Button2Up(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
	end
	
	function toggleAntiAFKFunction()
		if toggleAntiAFK then
			antiAFKConnection = game:GetService("Players").LocalPlayer.Idled:Connect(function()
				antiAFKAction()
			end)
		else
			if antiAFKConnection then
				antiAFKConnection:Disconnect()
			end
		end
	end
	
	function toggleAntiAFKVariable()
		toggleAntiAFK = not toggleAntiAFK
		warn("ANTI-AFK", toggleAntiAFK and "ENABLED" or "DISABLED")
		toggleAntiAFKFunction()
	end
	script.Parent.MouseButton1Click:Connect(function()
		if _G.Nitradenpgggggasaantf == false then
			_G.Nitradenpgggggasaantf = true
			script.Parent.Parent.checkerudeemn.BackgroundColor3 = Color3.fromRGB(255, 170, 0)
			toggleAntiAFKVariable()
		else
			_G.Nitradenpgggggasaantf = false
			script.Parent.Parent.checkerudeemn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
			toggleAntiAFK = false
		end
	end)
end
coroutine.wrap(WUNGFCC_fake_script)()
